generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  id        Int       @id @default(autoincrement())
  username  String
  title     String
  content   String
  createdat DateTime? @default(dbgenerated("CURRENT_DATE")) @db.Date
}
